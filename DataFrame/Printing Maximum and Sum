ID = c(1, 2, 3)
Name = c("ramu", "raju", "ravi")
Marks = c(50, 40, 25)
df = data.frame(ID, Name, Marks)
print(df)
ID = c(1, 2, 3)
Name = c("rau", "aju", "avi")
Marks = c(30, 20, 125)
df2 = data.frame(ID, Name , Marks)
df = rbind(df , df2)
print(df)
total_marks = sum(df$Marks)
print(total_marks)
max_mark = max(df$Marks)
print(max_mark)
index=which.max(df$Marks)
print(df$Name[index])